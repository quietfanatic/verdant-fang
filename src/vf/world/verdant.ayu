[ayu::Document {
    body: [vf::LayeredTexture [
        {
            image: assets.ayu#layer0
             -- y down
            bounds: [110 84 342 192]
        }
        {
            image: assets.ayu#layer1
             -- y down
            bounds: [110 84 342 192]
        }
        {
            image: assets.ayu#layer2
             -- y down
            bounds: [110 84 342 192]
        }
    ]]
     -- y up
    body_stand:      [vf::BodyFrame [#body [14 55] [0 23] [2 16]]]
    body_crouch:     [vf::BodyFrame [#body [60 61] [0 16] [1 13]]]
    body_walk0:      [vf::BodyFrame [#body [14 29] [1 23] [2 16]]]
    body_walk1:      [vf::BodyFrame [#body [37 29] [1 23] [2 16]]]
    body_walk2:      [vf::BodyFrame [#body [60 29] [1 22] [2 15]]]
    body_walk3:      [vf::BodyFrame [#body [83 29] [1 23] [2 16]]]
    body_walk4:      [vf::BodyFrame [#body [106 29] [1 23] [2 16]]]
    body_walk5:      [vf::BodyFrame [#body [129 29] [1 22] [2 15]]]
    body_fall1:      [vf::BodyFrame [#body [83 56] [1 23] [2 16]]]
    body_land:       [vf::BodyFrame [#body [37 57] [1 20] [2 14]]]
    body_preattack:  [vf::BodyFrame [#body [154 28] [0 23] [-2 19]]]
    body_attack:     [vf::BodyFrame [#body [172 28] [4 22] [16 19]]]
    body_postattack: [vf::BodyFrame [#body [203 28] [0 23] [-2 19]]]
    body_damage: [vf::BodyFrame [
        #body [106 54] [0 23] [0 17]
        [[0 13] [1 18] [0 22] [2 26]]
